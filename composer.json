{
    "name": "devnullius/yii2-i18n",
    "description": "Yii2 i18n module makes the translation of your application simple ",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "i18n",
        "translation",
        "translations"
    ],
    "homepage": "https://github.com/devnullius/yii2-i18n",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Creator"
        },
        {
            "name": "Aram H.",
            "email": "aram.ds@gmail.com",
            "role": "Improver"
        }
    ],
    "support": {
        "issues": "https://github.com/devnullius/yii2-i18n/issues",
        "source": "https://github.com/devnullius/yii2-i18n"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "process-timeout": 1800
    },
    "require": {
        "php": ">=7.4 < 8.0",
        "ramsey/uuid": "^4.1",
        "webmozart/assert": "^1.9",
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "devnullius\\i18n\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "devnullius\\i18n\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "bootstrap": "devnullius\\i18n\\Bootstrap"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
